{
  "name": "kogitoapp-gitea",
  "version": "1.0.4",
  "author": "Kogito UG <brainz+puppet@kogitoapp.com>",
  "summary": "A module to manage Gitea to enjoy self-hosted Git",
  "license": "Apache-2.0",
  "source": "https://github.com/kogitoapp/puppet-gitea.git",
  "project_page": "https://github.com/kogitoapp/puppet-gitea",
  "issues_url": "https://github.com/kogitoapp/puppet-gitea/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.14.0 < 5.0.0"
    },
    {
      "name": "puppetlabs-inifile",
      "version_requirement": ">= 1.6.0 < 3.0.0"
    },
    {
      "name": "lwf-remote_file",
      "version_requirement": ">= 1.1.3 < 2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
